{
	"name": "@svar-ui/svelte-grid",
	"version": "2.3.0",
	"description": "A fast, feature-rich Svelte DataGrid component",
	"productTag": "grid",
	"productTrial": false,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:dist": "vite build --mode dist",
		"build:tests": "vite build --mode test",
		"lint": "yarn eslint ./demos ./src",
		"start": "vite --open",
		"start:tests": "vite --open=/tests/ --host 0.0.0.0  --port 5100 --mode test",
		"test": "true",
		"test:cypress": "cypress run -P ./ --config \"baseUrl=http://localhost:5100/tests\""
	},
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js",
			"types": "./types/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/svar-widgets/grid.git"
	},
	"bugs": {
		"url": "https://forum.svar.dev"
	},
	"homepage": "https://svar.dev/svelte/datagrid/",
	"dependencies": {
		"@svar-ui/lib-dom": "0.9.2",
		"@svar-ui/lib-state": "1.9.6",
		"@svar-ui/lib-svelte": "0.5.2",
		"@svar-ui/svelte-menu": "2.3.0",
		"@svar-ui/svelte-core": "2.3.1",
		"@svar-ui/grid-data-provider": "2.3.0",
		"@svar-ui/grid-locales": "2.3.0",
		"@svar-ui/grid-store": "2.3.0"
	},
	"devDependencies": {
		"@svar-ui/svelte-editor": "2.3.0",
		"@svar-ui/svelte-comments": "2.3.0",
		"@svar-ui/svelte-tasklist": "2.3.0",
		"@svar-ui/svelte-filter": "2.3.0"
	},
	"files": [
		"src",
		"types",
		"readme.md",
		"whatsnew.md",
		"license.txt"
	],
	"keywords": [
		"svelte",
		"datagrid",
		"data table",
		"data-table",
		"data grid",
		"ui component",
		"datatable",
		"data-grid",
		"svelte datagrid",
		"svelte datatable",
		"svar datagrid",
		"svar widgets",
		"svelte-datagrid",
		"svelte-data-grid",
		"svelte-table"
	]
}
