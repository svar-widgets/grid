{
	"name": "wx-svelte-grid",
	"version": "2.1.1",
	"description": "A powerful and flexible DataGrid component written in Svelte",
	"productTag": "grid",
	"productTrial": false,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:dist": "vite build --mode dist",
		"build:tests": "vite build --mode test",
		"lint": "yarn eslint ./demos ./src",
		"start": "vite --open",
		"start:tests": "vite --open=/tests/ --host 0.0.0.0  --port 5100 --mode test",
		"test": "true",
		"test:cypress": "cypress run -P ./ --config \"baseUrl=http://localhost:5100/tests\""
	},
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/svar-widgets/grid.git"
	},
	"bugs": {
		"url": "https://forum.svar.dev"
	},
	"homepage": "https://svar.dev/svelte/datagrid/",
	"dependencies": {
		"wx-lib-dom": "0.8.0",
		"wx-lib-state": "1.9.0",
		"wx-lib-svelte": "0.5.1",
		"wx-svelte-menu": "2.1.0",
		"wx-svelte-core": "2.1.0",
		"wx-grid-data-provider": "2.1.1",
		"wx-grid-locales": "2.1.1",
		"wx-grid-store": "2.1.1"
	},
	"devDependencies": {
		"wx-svelte-editor": "2.1.0",
		"wx-svelte-comments": "2.1.0",
		"wx-svelte-tasklist": "2.1.0"
	},
	"files": [
		"src",
		"readme.md",
		"whatsnew.md",
		"license.txt"
	],
	"keywords": [
		"svelte",
		"datagrid",
		"data table",
		"data-table",
		"data grid",
		"ui component",
		"datatable",
		"data-grid",
		"svelte datagrid",
		"svelte datatable",
		"svar datagrid",
		"svar widgets",
		"svelte-datagrid",
		"svelte-data-grid",
		"svelte-table"
	]
}
